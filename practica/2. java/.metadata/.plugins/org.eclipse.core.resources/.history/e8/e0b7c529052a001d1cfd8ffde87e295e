package practicasAleatorias1;

public class practicasAleatorias1 {

	public static void main(String[] args) {
		
	/*	pg: 95
		5. Mostrar los m√∫ltiplos de 8 hasta el valor 500. Debe aparecer en pantalla 8 - 16 - 24, etc.
	*/
		
		int factorFijo=8;
		int factorVariante=0;
		
		while(factorFijo*factorVariante<=500) {
			System.out.println(factorFijo*factorVariante);
			factorVariante++;
		}
	}
}
